{
  "expo": {
    "name": "SINAAPP",
    "slug": "SINAAPP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashLogo.png",
        "backgroundColor": "#2B6128"
        
      },
      
        "splash": {
          "image": "./assets/images/splashLogo.png",
          "backgroundColor": "#2B6128"
        }
      
      
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splashLogo.png"
    },
    
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashLogo.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#2B6128"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
